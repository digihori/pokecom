<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00000000"
    android:columnCount="4"
    android:rowCount="4"
    android:id="@+id/keyFrame">


    <android.support.v7.widget.AppCompatButton
        android:text="sin"
        style="@style/charButton"
        android:layout_column="0"
        android:layout_row="0"
        android:id="@+id/buttonSIN"/>
    <android.support.v7.widget.AppCompatButton
        android:text="cos"
        style="@style/charButton"
        android:layout_column="1"
        android:layout_row="0"
        android:id="@+id/buttonCOS"/>
    <android.support.v7.widget.AppCompatButton
        android:text="tan"
        style="@style/charButton"
        android:layout_column="2"
        android:layout_row="0"
        android:id="@+id/buttonTAN"/>
    <android.support.v7.widget.AppCompatButton
        android:text="CE"
        style="@style/charButton"
        android:layout_column="3"
        android:layout_row="0"
        android:id="@+id/buttonCE"/>

    <android.support.v7.widget.AppCompatButton
        android:text="HEX"
        style="@style/charButton"
        android:layout_column="0"
        android:layout_row="1"
        android:id="@+id/buttonHEX"/>
    <android.support.v7.widget.AppCompatButton
        android:text="DEG"
        style="@style/charButton"
        android:layout_column="1"
        android:layout_row="1"
        android:id="@+id/buttonDEG"/>
    <android.support.v7.widget.AppCompatButton
        android:text="LN"
        style="@style/charButton"
        android:layout_column="2"
        android:layout_row="1"
        android:id="@+id/buttonLN"/>
    <android.support.v7.widget.AppCompatButton
        android:text="LOG"
        style="@style/charButton"
        android:layout_column="3"
        android:layout_row="1"
        android:id="@+id/buttonLOG"/>

    <android.support.v7.widget.AppCompatButton
        android:text="EXP"
        style="@style/charButton"
        android:layout_column="0"
        android:layout_row="2"
        android:id="@+id/buttonEXP"/>
    <android.support.v7.widget.AppCompatButton
        android:text="POW"
        style="@style/charButton"
        android:layout_column="1"
        android:layout_row="2"
        android:id="@+id/buttonPOW"/>
    <android.support.v7.widget.AppCompatButton
        android:text="ROOT"
        style="@style/charButton"
        android:layout_column="2"
        android:layout_row="2"
        android:id="@+id/buttonROOT"/>
    <android.support.v7.widget.AppCompatButton
        android:text="SQU"
        style="@style/charButton"
        android:layout_column="3"
        android:layout_row="2"
        android:id="@+id/buttonSQU"/>

    <android.support.v7.widget.AppCompatButton
        android:text="hyp"
        style="@style/charButton"
        android:layout_column="0"
        android:layout_row="3"
        android:id="@+id/buttonHYP"/>
    <android.support.v7.widget.AppCompatButton
        android:text="REC"
        style="@style/charButton"
        android:layout_column="1"
        android:layout_row="3"
        android:id="@+id/buttonREC"/>
    <android.support.v7.widget.AppCompatButton
        android:text="K1"
        style="@style/charButton"
        android:layout_column="2"
        android:layout_row="3"
        android:id="@+id/buttonK1"/>
    <android.support.v7.widget.AppCompatButton
        android:text="K2"
        style="@style/charButton"
        android:layout_column="3"
        android:layout_row="3"
        android:id="@+id/buttonK2"/>


</GridLayout>
