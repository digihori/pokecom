        style="@style/charButton"
        android:layout_column="5"
        android:layout_row="0"
        android:id="@+id/buttonRA"/>

    <android.support.v7.widget.AppCompatButton
        android:text="DEL"
        style="@style/charButton"
        android:layout_column="7"
        android:layout_row="0"
        android:id="@+id/buttonDEL"/>
