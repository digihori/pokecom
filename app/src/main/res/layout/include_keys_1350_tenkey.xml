<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00000000"
    android:columnCount="5"
    android:rowCount="4"
    android:id="@+id/keyFrame">


    <android.support.v7.widget.AppCompatButton
        android:text="7"
        style="@style/charButton"
        android:layout_column="0"
        android:layout_row="0"
        android:id="@+id/button7"/>
    <android.support.v7.widget.AppCompatButton
        android:text="8"
        style="@style/charButton"
        android:layout_column="1"
        android:layout_row="0"
        android:id="@+id/button8"/>
    <android.support.v7.widget.AppCompatButton
        android:text="9"
        style="@style/charButton"
        android:layout_column="2"
        android:layout_row="0"
        android:id="@+id/button9"/>
    <android.support.v7.widget.AppCompatButton
        android:text="("
        style="@style/charButton"
        android:layout_column="3"
        android:layout_row="0"
        android:id="@+id/buttonK1"/>
    <android.support.v7.widget.AppCompatButton
        android:text=")"
        style="@style/charButton"
        android:layout_column="4"
        android:layout_row="0"
        android:id="@+id/buttonK2"/>

    <!--          -->

    <android.support.v7.widget.AppCompatButton
        android:text="4"
        style="@style/charButton"
        android:layout_column="0"
        android:layout_row="1"
        android:id="@+id/button4"/>
    <android.support.v7.widget.AppCompatButton
        android:text="5"
        style="@style/charButton"
        android:layout_column="1"
        android:layout_row="1"
        android:id="@+id/button5"/>
    <android.support.v7.widget.AppCompatButton
        android:text="6"
        style="@style/charButton"
        android:layout_column="2"
        android:layout_row="1"
        android:id="@+id/button6"/>
    <android.support.v7.widget.AppCompatButton
        android:text="/"
        style="@style/charButton"
        android:layout_column="3"
        android:layout_row="1"
        android:id="@+id/buttonDIV"/>
    <android.support.v7.widget.AppCompatButton
        android:text=":"
        style="@style/charButton"
        android:layout_column="4"
        android:layout_row="1"
        android:id="@+id/buttonCLN"/>

    <!--          -->

    <android.support.v7.widget.AppCompatButton
        android:text="1"
        style="@style/charButton"
        android:layout_column="0"
        android:layout_row="2"
        android:id="@+id/button1"/>
    <android.support.v7.widget.AppCompatButton
        android:text="2"
        style="@style/charButton"
        android:layout_column="1"
        android:layout_row="2"
        android:id="@+id/button2"/>
    <android.support.v7.widget.AppCompatButton
        android:text="3"
        style="@style/charButton"
        android:layout_column="2"
        android:layout_row="2"
        android:id="@+id/button3"/>
    <android.support.v7.widget.AppCompatButton
        android:text="*"
        style="@style/charButton"
        android:layout_column="3"
        android:layout_row="2"
        android:id="@+id/buttonMLT"/>
    <android.support.v7.widget.AppCompatButton
        android:text=";"
        style="@style/charButton"
        android:layout_column="4"
        android:layout_row="2"
        android:id="@+id/buttonSCLN"/>

    <!--          -->

    <android.support.v7.widget.AppCompatButton
        android:text="0"
        style="@style/charButton"
        android:layout_column="0"
        android:layout_row="3"
        android:id="@+id/button0"/>
    <android.support.v7.widget.AppCompatButton
        android:text="."
        style="@style/charButton"
        android:layout_column="1"
        android:layout_row="3"
        android:id="@+id/buttonDOT"/>
    <android.support.v7.widget.AppCompatButton
        android:text="+"
        style="@style/charButton"
        android:layout_column="2"
        android:layout_row="3"
        android:id="@+id/buttonPLS"/>
    <android.support.v7.widget.AppCompatButton
        android:text="-"
        style="@style/charButton"
        android:layout_column="3"
        android:layout_row="3"
        android:id="@+id/buttonMINUS"/>
    <android.support.v7.widget.AppCompatButton
        android:text=","
        style="@style/charButton"
        android:layout_column="4"
        android:layout_row="3"
        android:id="@+id/buttonCOMMA"/>

</GridLayout>
